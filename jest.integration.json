{
  "verbose": true,
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "testMatch": [
    "**/__tests__/circe.test.js"
  ],
  "modulePathIgnorePatterns": [
    "dist/*"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ]
}
